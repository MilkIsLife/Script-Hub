function SCRIPT_QRFJ84_FAKESCRIPT() -- b6.6b 
	local script = Instance.new('LocalScript')
	script.Parent = b6
	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/TheRealPhantomX/Script-Hub/master/Counter%20Blox%20Aimbot", true))()
		script.Parent.Parent.Top.Title.Text = "Open The Link For Instructions!"
		wait(4)
		script.Parent.Parent.Top.Title.Text = "Phantom Hub"
	end)

end
coroutine.resume(coroutine.create(SCRIPT_QRFJ84_FAKESCRIPT))
