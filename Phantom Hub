-- Scripts:
function SCRIPT_QFFH77_FAKESCRIPT() -- _13.OC 
	local script = Instance.new('LocalScript')
	script.Parent = _13
	local button = script.Parent
	local toggled = false
	 
	local function onButtonActivated()
		if toggled == false then
			button.Parent.Parent.Main.Visible = true
			button.Text = "Close"
			toggled = true
		else		
			button.Parent.Parent.Main.Visible = false
			button.Text = "Open"
			toggled = false
		end
	end
	 
	button.Activated:Connect(onButtonActivated)

end
coroutine.resume(coroutine.create(SCRIPT_QFFH77_FAKESCRIPT))
function SCRIPT_DDEJ65_FAKESCRIPT() -- b2.2b 
	local script = Instance.new('LocalScript')
	script.Parent = b2
	script.Parent.MouseButton1Click:Connect(function()
	    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheRealPhantomX/Script-Hub/master/Phantom%20Hub", true))()
	end)

end
coroutine.resume(coroutine.create(SCRIPT_DDEJ65_FAKESCRIPT))
