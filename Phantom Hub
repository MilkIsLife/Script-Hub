function SCRIPT_WEKB68_FAKESCRIPT() -- Main.RGBControl 
	local script = Instance.new('LocalScript')
	script.Parent = Main
	while wait() do
	local r = (math.sin(workspace.DistributedGameTime/2)/2)+0.5
	local g = (math.sin(workspace.DistributedGameTime)/2)+0.5
	local b = (math.sin(workspace.DistributedGameTime*1.5)/2)+0.5
	local color = Color3.new(r, g, b)
	script.Parent.b1.BorderColor3 = color
	script.Parent.b2.BorderColor3 = color
	script.Parent.b3.BorderColor3 = color
	script.Parent.b4.BorderColor3 = color
	script.Parent.b5.BorderColor3 = color
	script.Parent.b6.BorderColor3 = color
	script.Parent.b7.BorderColor3 = color
	script.Parent.b8.BorderColor3 = color
	script.Parent.b9.BorderColor3 = color
	script.Parent.b10.BorderColor3 = color
	script.Parent.b11.BorderColor3 = color
	script.Parent.b12.BorderColor3 = color
	local r = (math.sin(workspace.DistributedGameTime/2)/2)+0.5
	local g = (math.sin(workspace.DistributedGameTime)/2)+0.5
	local b = (math.sin(workspace.DistributedGameTime*1.5)/2)+0.5
	end

end
coroutine.resume(coroutine.create(SCRIPT_WEKB68_FAKESCRIPT))
